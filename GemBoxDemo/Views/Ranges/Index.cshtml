@model RangesModel

<h1>
    Ranges Demo
</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Title</th>
            <th scope="col">Release Year</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var movie in Model.Movies)
        {
            <tr>
                <td>@movie.Title</td>
                <td>@movie.ReleaseYear?.ToString("yyyy")</td>
            </tr>
        }
    </tbody>
</table>

<a asp-controller="ranges" asp-action="download" class="btn btn-primary mt-3">
    Download
</a>